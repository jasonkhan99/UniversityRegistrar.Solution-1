@using System.Collections.Generic;
@using Registrar.Models;

@model List<Registrar.Models.Student>

@if (@Model.Count == 0)
{
  <h3>No students have enrolled yet!</h3>
} 

<ul>
  @foreach (Student student in Model)
  {
      <li>@Html.ActionLink(student.StudentName, "Details", new {id = student.StudentId})</li>
  }   
</ul>

@Html.ActionLink("Add a student", "Create", "Student")